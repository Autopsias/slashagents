{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/.*/issues/new"
    },
    {
      "pattern": "^https://github.com/.*/compare/"
    },
    {
      "pattern": "^https://claude.ai/"
    },
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "^#"
    }
  ],
  "replacementPatterns": [],
  "httpHeaders": [
    {
      "urls": ["https://github.com/"],
      "headers": {
        "Accept": "text/html"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "5s",
  "aliveStatusCodes": [200, 206, 301, 302, 307, 308]
}
